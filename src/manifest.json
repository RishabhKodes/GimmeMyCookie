{
  "name": "GMC",
  "version": "0.0.1",
  "description": "Removes all cookie warnings.",
  "content_scripts": [{
	"matches": ["http://*/*", "https://*/*"],
	"run_at": "document_idle",
	"js": ["contentScript.js"]
  }],
  "browser_action": {
	"default_popup": "popup.html"
  },
  "manifest_version": 2
}
